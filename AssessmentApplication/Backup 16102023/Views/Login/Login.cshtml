
@{
    ViewBag.Title = "Login";
    Layout = null;

}

<!DOCTYPE html>
<html lang="en">
<head>
    <base href="https://www.playtolearn.in/Assessment/">
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link href="~/Bootstrap/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Bootstrap/bootstrap.min.js"></script>
    <script src="~/Bootstrap/jquery.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.8.2/css/all.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Css/Style.css" rel="stylesheet" />
</head>
<body>
    <div class="container-fluid" style="height:100%;">
        <div class="container-fluid">
            <nav>
                <div class="logo">
                    <img src="~/Images/Coroebus.png" height="70" width="200" />

                </div>
            </nav>
        </div>

        <div class="wrapper">
            <div class="container">
                <div class="col-left">

                    <div class="login-text">

                        <img src="~/Images/-1.png" width="500" />
                    </div>
                </div>

                <div class="col-right">
                    <div class="login-form">
                        <form id="form-login">
                            <div class="text-center">

                                <img src="~/Images/MOLLY'STRIVIA.gif" style="margin-top: 0%; margin-bottom: 0%; " />
                            </div>

                            <div id="myAlert">
                            </div>
                            <div class="form-group" style="padding: 22px 0px 0px 0px;">
                                <input name="" id="Email" class="form-control" style="border-radius: 16px; height: 50px;" placeholder="User Name" type="email" required="required">
                            </div>
                            <div class="form-group" style="padding: 12px 0px 0px 0px;">
                                <input class="form-control" id="Password" style="border-radius:16px;height:50px;" placeholder="******" type="password">
                            </div>
                            <div class="form-group text-center" style=" padding: 17px 0px 0px 0px;">
                                <button type="button" id="btn" onclick="Login();" class="btn btn-block" style="color:#fff; background:#206b55;border-radius:16px;height:50px;"> Login  </button>
                                <br />
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <footer style="margin-top:-2.3%;">
            <p class="cp-text">
                © The Gamification Company.
            </p>
        </footer>
    </div>
    <script src="~/Javascript/JSFile.js"></script>


</body>
</html>
<script src="~/AppJavascript/Login.js"></script>
<script src="~/JavaScript/jquery.min.js"></script>









